{
    "server": {
        "name": "wRoot Server",
        "listen": "0.0.0.0",
        "port": 8000,
        "document_root": "WROOT_DOCUMENT_ROOT",
        "compressed_output": true,
        "threads": 8,
        "request_timeout": 120,
        "mapFilesToMemory": true
    },
    "url_rewrite": [
        {
            "input": "^(.+)$",
            "output": "$1"
        }
    ]
}